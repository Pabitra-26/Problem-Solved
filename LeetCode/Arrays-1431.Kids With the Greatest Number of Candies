class Solution:
    def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:
        l=[]
        m=max(candies)
        for i in candies:
            n=i+extraCandies
            if(n>=m):
                l.append(1)
            else:
                l.append(0)
        return l
        
